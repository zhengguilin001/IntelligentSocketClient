                <!-- 添加以下隐式启动action -->
                <action android:name="android.intent.action.CTYON_WXCHAT" />

                <category android:name="android.intent.category.DEFAULT" />
                <!-- 添加以上隐式启动action -->
            </intent-filter>
        </activity>
