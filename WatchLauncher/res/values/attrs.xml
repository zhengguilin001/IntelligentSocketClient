<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TabItem">
        <attr name="android:src"/>
        <attr name="tab_item_label" format="string"/>
        <attr name="tab_item_count" format="integer"/>
    </declare-styleable>

    <declare-styleable name="HorizontalVerticalViewPager">
        <attr name="isVertical" format="boolean" />
    </declare-styleable>
    <declare-styleable name="IconButton">
        <attr name="iconPadding" format="dimension"/>
    </declare-styleable>
</resources>
